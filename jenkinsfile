pipeline {
    agent any
    stages {
        stage('git repo & clean') {
            steps {
               bat "rmdir  /s /q booking"
                bat "git clone https://github.com/Chaayyma/Easy-booking1.git"
                bat "mvn clean -f booking"
            }
        }
        stage('install') {
            steps {
                bat "mvn install -f booking"
            }
        }
        stage('test') {
            steps {
                bat "mvn test -f booking"
            }
        }
        stage('package') {
            steps {
                bat "mvn package -f booking"
            }
        }
    }
}